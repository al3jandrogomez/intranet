<div class="container">
	<div class="form-group row">
		button type="button" class="btn btn-primary">Primary
		</button>

		<!-- Secondary, outline button -->
		<button type="button" class="btn btn-secondary">Secondary</button>

		<!-- Indicates a successful or positive action -->
		<button type="button" class="btn btn-success">Success</button>

		<!-- Contextual button for informational alert messages -->
		<button type="button" class="btn btn-info">Info</button>

		<!-- Indicates caution should be taken with this action -->
		<button type="button" class="btn btn-warning">Warning</button>

		<!-- Indicates a dangerous or potentially negative action -->
		<button type="button" class="btn btn-danger">Danger</button>

		<!-- Deemphasize a button by making it look like a link while maintaining button behavior -->
		<button type="button" class="btn btn-link">Link</button>
	</div>
	<div class="form-group row">
		<label for="exampleSelect1" class="col-1 col-form-label">Tipo
			de Documento</label>
		<div class="col-10">
			<select class="form-control" id="cmbTipoDocumento">
				<option>Factura</option>
				<option>Remision</option>
			</select>
		</div>
	</div>
	<div class="form-group row">
		<label for="example-search-input" class="col-1 col-form-label">Numero
			de Documento</label>
		<div class="col-10">
			<input class="form-control" type="text" value="" id="txtNoDocumento">
		</div>
	</div>
	<div class="form-group row">
		<label for="example-date-input" class="col-1 col-form-label">Fecha
			de Entrada</label>
		<div class="col-10">
			<input class="form-control" type="date" value="" id="txtFechaEntrada">
		</div>
	</div>
	<div class="form-group row">
		<label class="col-1 col-form-label" for="exampleSelect1">Proveedor</label>
		<div class="col-10">
			<select class="form-control" id="cmbProveedor">
				<option>Papeleria S.A.</option>
				<option>Almacen S.A.</option>
			</select>
		</div>
	</div>

	<div class="form-group row">
		<label for="example-date-input" class="col-1 col-form-label">Tipo
			de Inversion</label>
		<div class="col-10">
			<select class="form-control" id="cmbTipoInversion">
				<option>Fondo Revolvente</option>
				<option>Gasto Corriente</option>
			</select>
		</div>
	</div>

	<div class="form-group row">
		<label for="example-date-input" class="col-1 col-form-label">Programa</label>
		<div class="col-10">
			<select class="form-control" id="cmbPrograma">
				<option>Compra Directa</option>
				<option>Compra Normal</option>
			</select>
		</div>
	</div>

	<div class="form-group row">
		<label for="example-date-input" class="col-1 col-form-label">Obervaciones</label>
		<div class="col-10">
			<textarea class="form-control form-rounded" rows="3"></textarea>
		</div>
	</div>

	<div class="form-group row">
		<label for="example-search-input" class="col-1 col-form-label">Numero
			de Pedido</label>
		<div class="col-10">
			<input class="form-control" type="text" value="" id="txtNoPedido">
		</div>
	</div>

	<div class="form-group row">
		<label for="example-date-input" class="col-1 col-form-label">Fecha
			de Entrada</label>
		<div class="col-10">
			<input class="form-control" type="date" value="" id="txtFechaEntrada">
		</div>
	</div>
</div>


